{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Grew",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#variable"
		},
		{
			"include": "#comment"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.grew",
				"match": "\\b(package|pattern|without|rule|commands)\\b"
			}]
		},
		"variable": {
			"patterns": [{
				"name": "constant.language.grew",
				"match": "\\b(del_node|del_edge|add_edge|def_feat|shift|shift_in|shift_out|append_feats|prepend_feats)\\b"
			}]
		},
		"comment": {
			"patterns": [{
				"name": "comment.grew",
				"match": "%.*\n"
			}]
		},
		"strings": {
			"name": "string.quoted.double.grew",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.grew",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.grew"
}